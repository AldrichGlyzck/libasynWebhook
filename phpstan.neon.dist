parameters:
    level: max
    checkMissingIterableValueType: false
    paths:
        - .
    bootstrapFiles:
        - phar://vendor/PocketMine-MP.phar/vendor/autoload.php
    scanDirectories:
        - phar://vendor/PocketMine-MP.phar
    excludePaths:
        analyse:
            - bin
            - .github
            - vendor
            - libasynDiscordWebhook-PM/example-plugin
            - libasynDiscordWebhook-WDPE
            - libasynDiscordWebhook-Nukkit
    treatPhpDocTypesAsCertain: false